&lt;span style=&quot;box-sizing: border-box;&quot; class=&quot;pl-kos&quot;&gt;}&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;box-sizing: border-box;&quot; class=&quot;pl-kos&quot;&gt;}&lt;/span&gt;&lt;span style=&quot;box-sizing: border-box;&quot; class=&quot;pl-kos&quot;&gt;;&lt;/span&gt;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;box-sizing: border-box; color: var(--color-prettylights-syntax-keyword);&quot; class=&quot;pl-k&quot;&gt;let&lt;/span&gt; &lt;span style=&quot;box-sizing: border-box;&quot; class=&quot;pl-s1&quot;&gt;chart&lt;/span&gt; &lt;span style=&quot;box-sizing: border-box; color: var(--color-prettylights-syntax-constant);&quot; class=&quot;pl-c1&quot;&gt;=&lt;/span&gt; &lt;span style=&quot;box-sizing: border-box; color: var(--color-prettylights-syntax-keyword);&quot; class=&quot;pl-k&quot;&gt;new&lt;/span&gt; &lt;span style=&quot;box-sizing: border-box; color: var(--color-prettylights-syntax-variable);&quot; class=&quot;pl-v&quot;&gt;JsDiagramChart&lt;/span&gt;&lt;span style=&quot;box-sizing: border-box;&quot; class=&quot;pl-kos&quot;&gt;(&lt;/span&gt;&lt;span style=&quot;box-sizing: border-box;&quot; class=&quot;pl-s1&quot;&gt;dataExample&lt;/span&gt;&lt;span style=&quot;box-sizing: border-box;&quot; class=&quot;pl-kos&quot;&gt;)&lt;/span&gt;&lt;span style=&quot;box-sizing: border-box;&quot; class=&quot;pl-kos&quot;&gt;;&lt;/span&gt;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;box-sizing: border-box; color: var(--color-prettylights-syntax-comment);&quot; class=&quot;pl-c&quot;&gt;// window.addEventListener(&quot;resize&quot;, function() { chart = new JsDiagramChart(dataExample) });&lt;/span&gt;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;box-sizing: border-box; color: var(--color-prettylights-syntax-comment);&quot; class=&quot;pl-c&quot;&gt;// OTHERS METHODS&lt;/span&gt;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;box-sizing: border-box; color: var(--color-prettylights-syntax-comment);&quot; class=&quot;pl-c&quot;&gt;// chart.addZoom(-5);&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;box-sizing: border-box; color: var(--color-prettylights-syntax-comment);&quot; class=&quot;pl-c&quot;&gt;// chart.resetZoom();&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;box-sizing: border-box; color: var(--color-prettylights-syntax-comment);&quot; class=&quot;pl-c&quot;&gt;// chart.update(dataExample);&lt;/span&gt;&lt;/font&gt;&lt;/pre&gt;&lt;/div&gt;" style="text;whiteSpace=wrap;html=1;fontSize=12;" vertex="1" parent="1">
          <mxGeometry x="10" y="260" width="560" height="810" as="geometry" />
        </mxCell>
